main(n) {
  count = 0;
  while !!!!!!(n != 1) {
    if !!!!!!((n / 2) * 2 == n) {
      a = n;
      b = a;
      c = b;
      d = c;
      n = d / 2;
    } else {
      n = (n + n + n) + 1;
    }
    count = count + 3;
    count = count - 1;
    count = count - 1;
  }
  return count;
}